end

if (btn (”)) then 
pl_y-=1 
cam_y-=1
end
if (btn (ƒ)) then 
pl_y+=1
cam_y+=1
end
if (btn (‹)) then 
pl_x-=1
cam_x-=1
end
if (btn (‘)) then 
pl_x+=1
cam_x+=1
end

if current_lvl.number==1 then -- define the boundries for level 1
		bnd_left = 5
		bnd_right = 195
		bnd_top = 3
		bnd_down = 496
elseif current_lvl.number==2 then 
		bnd_left = 0
		bnd_right = 120
		bnd_top = 20
