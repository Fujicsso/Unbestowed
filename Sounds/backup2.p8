pico-8 cartridge // http://www.pico-8.com
version 14
__lua__
pl = {}
pl_x = 100  --player x
pl_y = 120  --player y --300
pl_color = 1 --cor
pl_sprite = 5 --sprite
pl_hit = 0
pl_tmr = 1
idlepl_tmr = 1
cam_x = 0
cam_y = 0
crack = false

cont = 60  --temporizador stuff
pl_tmp = 1
col = 10
ytmp=50  --change to 60
ytmpspeed=1
xtmp = 90
x__tmp = 90
x_tmp = 60

render = 30
item = true

world = {} -- initialize the level
world_x0 = 0 -- x starting position of the space the sprite will be in
world_y0 = 54 -- y starting position of the space the sprite will be in
world_x1 = 19 -- determines the width (100 - 10 = 90)
world_y1 = 64 -- determines the height of the space (40 - 10 = 30)
bnd_left = 0
bnd_right = 0
bnd_top = 0
bnd_down = 0

b1_x = 80
b1_y = 80
b2_x = 90
b2_y = 82
b3_x = 100
b3_y = 84
b4_x = 110
b4_y = 82
b5_x = 120
b5_y = 80
spawned_balls = false
balls_timer = 0
balls = {} --balls array

current_lvl = {} -- holder for the level counter
current_lvl.number = 0 -- initialize the level at 0



function init_balls()
	if initballs == false then 
	b1_x = 80
 b1_y = 80
	b2_x = 90
	b2_y = 82
	b3_x = 100
	b3_y = 84
	b4_x = 110
	b4_y = 82
	b5_x = 120
	b5_y = 80
	initballs = true
	end
end

function _spawnballs()
	
	ball1= spr (14, b1_x, b1_y)
	ball2= spr (14, b2_x, b2_y)
	ball3= spr (14, b3_x, b3_y)
	ball4= spr (14, b4_x, b4_y)
	ball5= spr (14, b5_x, b5_y)
	
	add(balls, ball1)
	add(balls, ball2)
	add(balls, ball3)
	add(balls, ball4)
	add(balls, ball5)
	
	
	spawned_balls = true
	
	
end

function _onhit()
		if pl_hit == 0 then
			return false
		elseif pl_hit == 1 then
			if not crack then
			cont -= 20
			crack = true
		end
			return true
		end
	 
end

function _gameover()
go_text = "game over"
end

function lvl_change(ln)
    current_lvl.number = ln
    if current_lvl.number==1 then -- code for only level 1
        pl_x = 100
        pl_y = 120 --change to 300
    elseif current_lvl.number==2 then
    				pl_x = ((world_x0 + world_x1) /2) - 8 
        pl_y = world_y1 - 15
    elseif current_lvl.number==3 then
    				pl_x = ((world_x0 + world_x1) /2) - 8 
        pl_y = world_y1 - 15
    elseif current_lvl.number==4 then
    				pl_x = ((world_x0 + world_x1) /2) - 8 
        pl_y = world_y1 - 15
    end
end

function _init()
    cls() -- clear the screen
    lvl_change(1) -- start the game at level 1
end



cam_x = 36 --camera x --change to 36
cam_y = 66 --camera y --change to 236


function sprite_animator(x)
local y = 0 
	y += x
	return y
end

function draw_lvl() -- abstracting the built in draw function to render the required level
	if current_lvl.number==1 then 
		map(38, 0,0, 0,26,64)
  camera(cam_x,cam_y)
  end
end

function _draw() -- write pixels to view at 30fps
	cls() -- clear thew screen
	draw_lvl() -- level rendering
	spr(pl_sprite,pl_x,pl_y) -- draw the main sprite with the modified sprite properties 


	print ("x "..pl_x,pl_x+10, pl_y, 10)
	print ("y "..pl_y,pl_x+40, pl_y, 10)

	if cont >= 1 then
print ("time:",(pl_x + 25),(pl_y-ytmp),col)
print (cont,(pl_x +45),(pl_y-ytmp),col)
else print (go_text, (pl_x +28), (pl_y-ytmp), col)
print ("time:",(pl_x + 8),(pl_y-ytmp),col)
end

	_spawnballs()
end

function _update()
if (not _onhit()) then
 pl_tmr += 1
 if (btn (”)) then
 pl_sprite = 11
 	if pl_tmr>=5 then
 	pl_sprite = 12
 end
